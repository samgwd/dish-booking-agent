# Keys
OPENAI_API_KEY=<your-api-key>
GOOGLE_OAUTH_CREDENTIALS=<path-to-your-google-oauth-creds>

# DiSH MCP Setup
DISH_MCP_PATH=<your-dish-mcp-path>
DISH_COOKIE=<your-dish-cookie>
TEAM_ID=<your-team-id>
MEMBER_ID=<your-member-id>

# Backend 
FASTAPI_URL=http://127.0.0.1:8000
DATABASE_URL=postgresql+psycopg2://dish_user:<your_db_password>@localhost:5433/dish_booking_user

# Keycloak (required for backend auth; service will fail fast if missing)
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_REALM=dish-booking-agent
KEYCLOAK_CLIENT_ID=dish-booking-agent-api

# User Secrets Encryption
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
SECRETS_ENCRYPTION_KEY=<your-generated-fernet-key>
