# =============================================================================
# DiSH Booking Agent - Environment Variables
# =============================================================================
# Copy this file to .env and fill in your values:
#   cp example.env .env
# =============================================================================

# -----------------------------------------------------------------------------
# Keycloak Configuration
# -----------------------------------------------------------------------------
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_REALM=dish-booking-agent
KEYCLOAK_CLIENT_ID=dish-booking-agent-api

KEYCLOAK_POSTGRES_DB=keycloak
KEYCLOAK_POSTGRES_USER=keycloak
KEYCLOAK_POSTGRES_PASSWORD=change-me-keycloak-db

KEYCLOAK_BOOTSTRAP_ADMIN_USERNAME=admin
KEYCLOAK_BOOTSTRAP_ADMIN_PASSWORD=change-me-keycloak-admin

# User Secrets Encryption
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
SECRETS_ENCRYPTION_KEY=<your-generated-fernet-key>

# -----------------------------------------------------------------------------
# User Database Configuration
# -----------------------------------------------------------------------------
POSTGRES_DB=dish_booking_user
POSTGRES_USER=dish_user
POSTGRES_PASSWORD=change-me-user-db

# -----------------------------------------------------------------------------
# Backend API Configuration
# -----------------------------------------------------------------------------
FASTAPI_URL=http://127.0.0.1:8000
DATABASE_URL=postgresql+psycopg2://dish_user:change-me-user-db@localhost:5433/dish_booking_user

# -----------------------------------------------------------------------------
# OpenAI / LLM Configuration
# -----------------------------------------------------------------------------
OPENAI_API_KEY=<your-openai-api-key>

# -----------------------------------------------------------------------------
# DiSH MCP Configuration
# -----------------------------------------------------------------------------
DISH_MCP_PATH=<absolute-path-to-dish-mcp-repo>
DISH_COOKIE=<your-dish-cookie>
TEAM_ID=<your-team-id>
MEMBER_ID=<your-member-id>

# -----------------------------------------------------------------------------
# Google Calendar MCP Configuration
# -----------------------------------------------------------------------------
GOOGLE_OAUTH_CREDENTIALS=<absolute-path-to-google-oauth-json>
